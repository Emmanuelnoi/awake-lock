{
  "name": "awake-lock",
  "version": "1.2.0",
  "type": "module",
  "description": "A comprehensive wake lock library for preventing device sleep with intelligent fallback strategies and exceptional performance",
  "main": "dist/cjs/index.core.js",
  "module": "dist/esm/index.core.js",
  "umd": "dist/umd/awake-lock.js",
  "types": "./dist/esm/index.core.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.core.d.ts",
      "import": "./dist/esm/index.core.js",
      "require": "./dist/cjs/index.core.js"
    },
    "./core": {
      "types": "./dist/esm/index.core.d.ts", 
      "import": "./dist/esm/index.core.js",
      "require": "./dist/cjs/index.core.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:prod": "NODE_ENV=production rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit --skipLibCheck || true",
    "clean": "rimraf dist",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "pnpm run build",
    "preversion": "pnpm test",
    "postversion": "pnpm publish"
  },
  "keywords": [
    "wake-lock",
    "screen-wake-lock",
    "prevent-sleep",
    "typescript",
    "react",
    "vue",
    "angular",
    "battery-optimization",
    "performance",
    "browser-compatibility",
    "fallback-strategies",
    "optimized"
  ],
  "author": "Emmanuel Noi",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.31.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.45.1",
    "rollup-plugin-filesize": "^10.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@angular/core": ">=12.0.0",
    "react": ">=16.8.0",
    "rxjs": ">=6.0.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "@angular/core": {
      "optional": true
    },
    "rxjs": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Emmanuelnoi/awake-lock.git"
  },
  "bugs": {
    "url": "https://github.com/Emmanuelnoi/awake-lock/issues"
  },
  "homepage": "https://github.com/Emmanuelnoi/awake-lock#readme"
}
